// create a pattern 

#include<stdio.h>
#include<conio.h>

int main() 
{
    int i,j,n;
    printf("Enter a number: ");
    scanf("%d",&n);
    
    for(i=1; i<=n; i++)
    {
        for(j=1; j<=n; j++)
        {
            if(i%2 == 0)
                printf("%c",64+i);
            else 
            printf("%c",96+i);
        }
        printf("\n");
    }
}
