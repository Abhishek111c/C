{
 GoodMorning();
 GoodAfternoon();
 GoodEvening();
    return 0;
}

 void GoodMorning(){
     printf("GoodMorning Abhishek Chaudhary\n");
 }
 
  void GoodAfternoon(){
     printf("GoodAfternoon Abhishek Chaudhary\n");
 }
 
